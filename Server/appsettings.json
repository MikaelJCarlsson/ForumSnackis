{
    "ConnectionStrings": {
        "DefaultConnection": "Server={YourServer};Initial Catalog={YourDB};Persist Security Info=False;User ID={YourID};Password={YourPassword};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"

    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "IdentityServer": {
        "Key": {
            "Type": "Store",
            "StoreName": "My",
            "StoreLocation": "CurrentUser",
            "Name": "CN={YourCert}"
        },
            "Clients": {
                "ForumSnackis.Client": {
                    "Profile": "IdentityServerSPA"
                }
            }
        },

        "AllowedHosts": "*"
    }